<?xml version="1.0"?>
<launch>

  <arg name="rviz" default="true" />


  <node pkg="parking_car" type="ndt_mapping_2d" name="ndt_mapping_2d" output="screen" >
  </node>


<group if="$(arg rviz)">
    <node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find parking_car)/rviz_cfg/ndt_mapping_2d.rviz" />
  </group>


</launch>
